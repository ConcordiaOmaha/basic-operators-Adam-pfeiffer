//Prime number Search Engine
#include <iostream>
#include<math.h>
using namespace std;
int main()
//Loop start
{
    for(int i=1; i<=3; i++){
    
	int num1,num2;
    int fnd=0,ctr=0;
	cout << "--Enter a number for starting range: ";
	cin>> num1;
	cout << " Enter a number for ending range: ";
	cin>> num2;		

	cout << "\n The prime numbers between "<<num1<<" and "<<num2<<" is :"<<endl;
    for(int i=num1;i<=num2;i++)
       {
           for(int j=2;j<=sqrt(i);j++)
               {
               if(i%j==0)
                  ctr++;
               }
               if(ctr==0&&i!=1)
               { fnd++;
                 cout<<i<<" ";
                 ctr=0;
               }
               ctr=0;
       }
}
}
